{
  "cacheDirectory": ".cache",
  "presets": [
    "babel-preset-react",
    "babel-preset-es2015",
    "babel-preset-stage-0"
  ],
  "plugins": [],
  "env": {
    "development": {
      "presets": [
        "babel-preset-react-hmre"
      ],
      "plugins": [
        "react-hot-loader/babel"
      ]
    },
    "production": {
      "compact": false,
      "comments": false,
      "plugins": [
        "babel-plugin-transform-runtime"
      ]
    }
  }
}